<div>
    <h1>Dashboard</h1>
    {% if app.user %}
        <p>Hello {{ app.user.username }}! Your email is {{ app.user.email|default('"unknown"') }}</p>
        <a href="{{ logout_path }}">Logout</a>
    {% else %}
        <ul>


            <li><a href="{{ login_paths.google }}">Login with Google</a></li>

        </ul>
    {% endif %}
</div>